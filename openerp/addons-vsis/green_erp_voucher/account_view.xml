	        <field name="name">green.erp.voucher.search</field>
	        <field name="model">green.erp.voucher</field>
	        <field name="arch" type="xml">
	            <search string="Thu/Chi">
	                <field name="user_id"/>
	                <field name="date"/>
					<field name="project_id"/>
